{
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": "3",
        "version": "^7.14"
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current",
          "esmodules": true
        },
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": { "version": "3.12.1", "proposals": true }
      }
    ],
    ["airbnb", { "runtimeVersion": "^7.14" }]
  ]
}
