{
  "name": "discord-server-bot",
  "private": true,
  "main": "dist/index.js",
  "author": "Ryan Murulo",
  "repository": "github:GetUsernameFromDatabase/discord-server-bot",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "clean": "yarn dlx rimraf dist",
    "build:index": " yarn vscode-generate-index-standalone src",
    "build": "yarn build:index && swc src -d dist -s",
    "build:clean": "yarn clean && yarn build",
    "watch": "swc src -d dist -w",
    "start": "node dist/index.js",
    "start:watch": "run-p watch start",
    "start:pm2": "yarn build && yarn start:watch",
    "dev": "yarn build:clean && yarn start",
    "dev:watch": "yarn build:clean && yarn start:watch",
    "dev:piece": "node -r @swc-node/register development/index.ts"
  },
  "dependencies": {
    "@discord-player/equalizer": "^0.2.3",
    "@discord-player/extractor": "^4.5.1",
    "@discord-player/utils": "^0.2.2",
    "@distube/ytdl-core": "^4.13.3",
    "@sapphire/discord.js-utilities": "^7.3.3",
    "@sapphire/duration": "^1.2.0",
    "@sapphire/fetch": "^3.0.5",
    "@sapphire/framework": "^5.3.6",
    "@sapphire/plugin-api": "^6.1.1",
    "@sapphire/plugin-hmr": "^3.0.2",
    "@sapphire/plugin-logger": "^4.1.0",
    "@sapphire/time-utilities": "^1.7.14",
    "@skyra/env-utilities": "^1.3.0",
    "better-sqlite3": "^9.6.0",
    "cache-manager": "^5.7.6",
    "cheerio": "^1.1.2",
    "colorette": "^2.0.20",
    "cronstrue": "^2.59.0",
    "discord-api-types": "^0.37.67",
    "discord-player": "^6.7.1",
    "discord.js": "^14.22.1",
    "kysely": "^0.28.5",
    "mediaplex": "^0.0.9",
    "node-html-markdown": "^1.3.0",
    "opusscript": "^0.1.1",
    "youtube-ext": "^1.1.25"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^5.0.1",
    "@swc-node/register": "^1.11.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/eslint": "^8.56.12",
    "@types/node": "^20.19.13",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.3.3",
    "vscode-generate-index-standalone": "^1.7.1"
  },
  "packageManager": "yarn@4.9.4"
}
