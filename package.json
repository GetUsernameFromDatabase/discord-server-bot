{
  "main": "lib/index.js",
  "name": "my-discord-server-bot",
  "author": "Ryan Kruberg",
  "description": "Discord bot that I'm making for a discord server I'm co-owner of",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "repository": "github:GetUsernameFromDatabase/discord-server-bot",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "start": "node --trace-warnings -r dotenv/config ./src/index.js",
    "build": "babel src/ -d lib/",
    "lint": "eslint .",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "displayName": "My Discord Server Bot",
    "verbose": true,
    "bail": true,
    "errorOnDeprecated": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/runtime": "^7.17.7",
    "@babel/runtime-corejs3": "^7.17.7",
    "@babel/types": "^7.17.0",
    "@discord-player/extractor": "^3.0.2",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "axios": "^0.26.1",
    "babel-plugin-flow-to-typescript": "^0.6.0",
    "cheerio": "^1.0.0-rc.10",
    "discord-player": "^5.2.2",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "ffmpeg-static": "^5.0.0",
    "npm": "^8.5.4",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@jest/globals": "^27.5.1",
    "@types/jest": "^27.4.1",
    "babel-jest": "^27.5.1",
    "babel-preset-airbnb": "^5.0.0",
    "core-js": "^3.21.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "prettier": "^2.5.1"
  }
}
