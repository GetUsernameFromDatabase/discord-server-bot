{
  "main": "lib/index.js",
  "name": "my-discord-server-bot",
  "author": "Ryan Kruberg",
  "description": "Discord bot that I'm making for a discord server I'm co-owner of",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "repository": "github:GetUsernameFromDatabase/discord-server-bot",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "start": "node --trace-warnings -r dotenv/config ./src/Main.js",
    "build": "babel src/ -d lib/",
    "lint": "eslint .",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "displayName": "My Discord Server Bot",
    "verbose": true,
    "bail": true,
    "errorOnDeprecated": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@babel/runtime-corejs3": "^7.15.3",
    "@babel/types": "^7.15.0",
    "axios": "^0.21.1",
    "babel-plugin-flow-to-typescript": "^0.6.0",
    "cheerio": "^1.0.0-rc.10",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "turndown": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@jest/globals": "^27.0.3",
    "@types/jest": "^26.0.23",
    "babel-jest": "^27.0.2",
    "babel-preset-airbnb": "^5.0.0",
    "core-js": "^3.16.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "jest": "^27.0.4",
    "jest-extended": "^0.11.5",
    "prettier": "^2.3.1"
  }
}
