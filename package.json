{
  "name": "my-discord-server-bot",
  "author": "Ryan Kruberg",
  "description": "Discord bot that I'm making for a discord server I'm co-owner of",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "repository": "github:GetUsernameFromDatabase/discord-server-bot",
  "private": true,
  "type": "module",
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=18.15.0"
  },
  "main": "src/index.ts",
  "scripts": {
    "start": "yarn make:index && tsc -b --clean && tsc && node dist/index.js",
    "dev": "yarn make:index && ts-node src/index.ts",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "lint:fix:notests": "yarn lint:fix --ignore-pattern __tests__ --ignore-pattern __mocks__",
    "test": "jest --forceExit --detectOpenHandles",
    "make:index": " yarn vscode-generate-index-standalone src"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "displayName": "My Discord Server Bot",
    "verbose": true,
    "bail": true,
    "errorOnDeprecated": true,
    "testEnvironment": "node"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.1.2",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.15.0",
    "axios": "^1.3.4",
    "cheerio": "^1.0.0-rc.12",
    "discord-player": "^6.1.1",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "node-fetch": "^3.3.1",
    "npm": "^9.6.2",
    "play-dl": "^1.9.6",
    "pm2": "^5.3.0",
    "slash-create": "^5.11.0",
    "soundcloud-scraper": "^5.0.3",
    "spotify-url-info": "^3.2.3",
    "turndown": "^7.1.2",
    "youtube-sr": "^4.3.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/async": "^3.2.18",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^40.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.13.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vscode-generate-index-standalone": "^1.7.1"
  }
}
